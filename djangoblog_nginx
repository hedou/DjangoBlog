server {

    listen 8080;

    keepalive_timeout 70;
    #access_log /var/log/nginx/django_access.log;
    #error_log /var/log/nginx/django_error.log;

    location / {
         
        

        proxy_pass http://127.0.0.1:8000;

        
    }
    
    location /static/ {
          expires max;
          root /root/test/DjangoBlogcollectedstatic/;
    }

    

}